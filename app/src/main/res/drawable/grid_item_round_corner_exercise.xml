<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/exercise_grid_item_color"> </solid>
            <stroke android:width="1dp" android:color="@color/startColorScreenBackgroundGradient"></stroke>
            <corners
                android:bottomRightRadius="3dp"
                android:bottomLeftRadius="3dp"
                android:topLeftRadius="3dp"
                android:topRightRadius="3dp" />
        </shape>
    </item>
</layer-list>